<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="nga.uit.edu.mytravel">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MyTravel">
        <activity android:name="controller.minh.ChangePasswordActivity"></activity>
        <activity android:name="controller.minh.ChangeProfileActivity" />
        <activity android:name="controller.minh.ProfileActivity" />
        <activity android:name="controller.mytravel.RegisterActivity" />
        <activity android:name="controller.mytravel.LoginActivity" />
        <activity android:name="controller.mytravel.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>